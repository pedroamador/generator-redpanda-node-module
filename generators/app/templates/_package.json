{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "dependencies": {
    "module-alias": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.1",
    "coveralls": "^2.13.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "http-server": "^0.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "mocha": "^4.1.0",
    "nyc": "^11.0.2",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "standard": "^10.0.3",
    "validate-commit": "^3.2.1"
  },
  "scripts": {
    "commitmsg": "validate-commit-msg -p eslint",
    "lint": "eslint */**/*.js -f html -o ./test/report/linter/index.html",
    "test": "node test/index.js",
    "cover":
      "nyc  --reporter=lcov --report-dir=test/reports/coverage --temp-directory=test/reports/coverage npm test",
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "serve:coverage": "http-server test/reports/coverage/ -o",
    "serve:linter": "http-server test/report/linter"
  },
  "repository": {
    "type": "git",
    "url": "git+<%= gitrepository %>"
  },
  "keywords": ["<%= keywords %>"],
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>",
    "url": "<%= url %>"
  },
  "license": "MIT",
  "bugs": {
    "url": "<%= gitrepository %>/issues"
  },
  "homepage": "<%= gitrepository %>#readme"
}
